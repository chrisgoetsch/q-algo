// File: .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: General Debug",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Run Q-ALGO Live",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/run_q_algo_live.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
